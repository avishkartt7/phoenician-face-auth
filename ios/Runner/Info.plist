		<key>MLKitModelDownloadConditionWifi</key>
		<true/>
		<key>MLKitModelDownloadConditionCharging</key>
		<false/>
		<key>MLKitModelDownloadConditionDeviceIdle</key>
		<false/>
	</dict>
	
